{
    "name": "symplify/sniffer-fixer-to-ecs-converter",
    "description": "Converter PHP_CodeSniffer or PHP-CS-Fixer configs to ECS with single Command",
    "license": "MIT",
    "bin": [
        "bin/sniffer-fixer-to-ecs-converter"
    ],
    "require": {
        "php": ">=7.3",
        "ext-simplexml": "*",
        "nette/utils": "^3.2",
        "nette/robot-loader": "^3.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^4.4|^5.3",
        "symfony/yaml": "^4.4|^5.3",
        "symfony/http-kernel": "^4.4|^5.3",
        "symplify/php-config-printer": "^9.3.26",
        "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symplify/symplify-kernel": "^9.3.26"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SnifferFixerToECSConverter\\": "src"
        },
        "files": [
            "stubs/Sniff.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SnifferFixerToECSConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
